// Programa de prueba: factorial iterativo
program factorial;
var i;
var res;
begin
    writec "Escriba un numero: ";
    read i;
    writec "Valor de i: ";
    write i;
    writeln;
    writec "Se va a calcular el factorial";
    writeln;
    res := 1;
    while i >= 1 do begin
        res := res * i;
        i := i - 1;
    end
    writec "Resultado: ";
    write res;
    writeln;
end.